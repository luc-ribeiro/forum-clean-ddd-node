{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "standard-with-typescript",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-useless-constructor": "off",
        "@typescript-eslint/no-useless-constructor": "off",
        "@typescript-eslint/explicit-function-return-type": "off"
    }
}
